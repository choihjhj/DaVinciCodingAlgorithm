# 풀이1) DFS
import sys
sys.setrecursionlimit(10**4)
N=int(sys.stdin.readline().rstrip())
M=int(sys.stdin.readline().rstrip())
arr=[[] for _ in range(N+1)] #맨앞 빈배열[]
for _ in range(M):
  u, v = map(int, sys.stdin.readline().rstrip().split())
  arr[u].append(v)
  arr[v].append(u)

visited = [False] * (N + 1)
def dfs(node):
    visited[node] = True
    cnt=1
    for next_node in arr[node]:
        if not visited[next_node]:
            cnt+=dfs(next_node)
    return cnt
print(dfs(1)-1) #자기자신 빼기

# 풀이2) DFS 2
import sys
sys.setrecursionlimit(10**4)
N=int(sys.stdin.readline().rstrip())
M=int(sys.stdin.readline().rstrip())

arr=[[] for _ in range(N+1)] #맨앞 빈배열[]
for _ in range(M):
  u, v = map(int, sys.stdin.readline().rstrip().split())
  arr[u].append(v)
  arr[v].append(u)

visited = [False] * (N + 1)
cnt=0
def dfs(node):
    visited[node] = True
    global cnt
    for next_node in arr[node]:
        if not visited[next_node]:
            dfs(next_node)
            cnt+=1
dfs(1)    
print(cnt) 

# 풀이3) BFS
import sys
from collections import deque
N=int(sys.stdin.readline().rstrip())
M=int(sys.stdin.readline().rstrip())
arr=[[] for _ in range(N+1)] #맨앞 빈배열[]
for _ in range(M):
  u, v = map(int, sys.stdin.readline().rstrip().split())
  arr[u].append(v)
  arr[v].append(u)

visited = [False] * (N + 1)
def bfs(node):
    dq=deque([node])
    visited[node]=True
    cnt=0
    while dq:
        v=dq.popleft()
        for next_node in arr[v]:
            if not visited[next_node]:
                dq.append(next_node)
                visited[next_node]=True
                cnt+=1
    return cnt
print(bfs(1))
